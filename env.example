# AgentFlow Environment Configuration

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================
DATABASE_URL=postgresql://agentflow:agentflow@localhost:5432/agentflow
SUPABASE_URL=your-supabase-url
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# =============================================================================
# REDIS CONFIGURATION
# =============================================================================
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=

# =============================================================================
# SECURITY CONFIGURATION
# =============================================================================
SECRET_KEY=your-secret-key-here-change-in-production
JWT_SECRET=your-jwt-secret-here-change-in-production
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30
JWT_REFRESH_TOKEN_EXPIRE_DAYS=7
ENCRYPTION_KEY=your-encryption-key-here

# =============================================================================
# API CONFIGURATION
# =============================================================================
API_V1_STR=/api/v1
PROJECT_NAME=AgentFlow
VERSION=1.0.0
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO

# =============================================================================
# CORS CONFIGURATION
# =============================================================================
BACKEND_CORS_ORIGINS=["http://localhost:3000","https://localhost:3000","http://localhost","https://localhost"]
ALLOWED_HOSTS=["localhost","127.0.0.1","0.0.0.0"]

# =============================================================================
# RATE LIMITING
# =============================================================================
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS_PER_MINUTE=60
RATE_LIMIT_BURST=10

# =============================================================================
# AI/LLM CONFIGURATION
# =============================================================================
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
LANGCHAIN_API_KEY=your-langchain-api-key

# =============================================================================
# EXTERNAL SERVICES
# =============================================================================
# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your-email@gmail.com
SMTP_PASSWORD=your-app-password
SMTP_TLS=true
SMTP_SSL=false

# File Storage
STORAGE_TYPE=local  # local, s3, gcs
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_S3_BUCKET=agentflow-storage
AWS_REGION=us-east-1

# =============================================================================
# MONITORING & LOGGING
# =============================================================================
MONITORING_ENABLED=true
PROMETHEUS_ENABLED=true
GRAFANA_ENABLED=true
SENTRY_DSN=your-sentry-dsn
LOG_FORMAT=json
AUDIT_LOGGING_ENABLED=true

# =============================================================================
# WORKFLOW EXECUTION
# =============================================================================
MAX_CONCURRENT_EXECUTIONS=100
EXECUTION_TIMEOUT_SECONDS=3600
AGENT_TIMEOUT_SECONDS=300
MAX_RETRIES=3
RETRY_DELAY_SECONDS=5

# =============================================================================
# WEBSOCKET CONFIGURATION
# =============================================================================
WS_HEARTBEAT_INTERVAL=30
WS_CONNECTION_TIMEOUT=60
WS_MAX_CONNECTIONS_PER_USER=5

# =============================================================================
# FRONTEND CONFIGURATION
# =============================================================================
VITE_API_URL=http://localhost:8000
VITE_WS_URL=ws://localhost:8000
VITE_APP_NAME=AgentFlow
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Visual Multi-Agent Workflow Designer

# =============================================================================
# DEVELOPMENT CONFIGURATION
# =============================================================================
# Set to true for development
HOT_RELOAD=true
AUTO_MIGRATE=true
CREATE_SAMPLE_DATA=true
ENABLE_DEBUG_TOOLBAR=true

# =============================================================================
# PRODUCTION CONFIGURATION
# =============================================================================
# Set these for production deployment
# ENVIRONMENT=production
# DEBUG=false
# LOG_LEVEL=WARNING
# RATE_LIMIT_ENABLED=true
# MONITORING_ENABLED=true
# AUDIT_LOGGING_ENABLED=true








